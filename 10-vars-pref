- name: variables pref
  hosts: localhost 
  vars:
    WISHES: "WISHES FROM PLAYLEVEL"
  vars_files:
    vars.yaml  
  vars_prompt:
  - name: WISHES
    prompt: Please enter WISHES from vars
    private: false  
  tasks:
  - name: Check pref
    vars:
      WISHES: "WISHES FROM TASK LEVEL"
    ansible.builtin.debug:
      msg: "Hello, {WISHES}"